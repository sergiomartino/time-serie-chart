{
  "name": "time-serie",
  "version": "1.0.0",
  "description": "Time serie chart with stock search and date range",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./index.html",
    "build": "npm-run-all build:clean build:run",
    "build:run": "parcel build ./index.html",
    "build:clean": "rimraf ./dist",
    "test": "npm-run-all test:unit test:render build test:e2e",
    "test:unit": "jest ./tests/unit",
    "test:render": "jest ./tests/render",
    "test:e2e": "testcafe chrome:headless ./tests/e2e/**/*.e2e.js --app \"http-server ./dist -s -p 8350\""
  },
  "config": {
    "e2eTestHost": "http://localhost:8350"
  },
  "author": "Sergio Martino",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "babel-jest": "^26.6.3",
    "http-server": "^0.12.3",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "react-test-renderer": "^16.14.0",
    "rimraf": "^2.7.1",
    "testcafe": "^1.11.0"
  },
  "dependencies": {
    "lodash": "^4.17.20",
    "react": "^16.8.6",
    "react-charts": "^2.0.0-beta.7",
    "react-dom": "^16.8.6",
    "react-select": "^4.1.0"
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 4
  },
  "engines": {
    "node": ">=14"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  }
}
